#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/socket-io/client/node_modules/.pnpm/ejs@3.1.8/node_modules/ejs/bin/cli.js/node_modules:/home/socket-io/client/node_modules/.pnpm/ejs@3.1.8/node_modules/ejs/bin/node_modules:/home/socket-io/client/node_modules/.pnpm/ejs@3.1.8/node_modules/ejs/node_modules:/home/socket-io/client/node_modules/.pnpm/ejs@3.1.8/node_modules:/home/socket-io/client/node_modules/.pnpm/node_modules:/home/socket-io/client/node_modules:/home/socket-io/node_modules:/home/node_modules:/node_modules:/home/socket-io/client/node_modules/ejs/bin/cli.js/node_modules:/home/socket-io/client/node_modules/ejs/bin/node_modules:/home/socket-io/client/node_modules/ejs/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ejs/bin/cli.js" "$@"
else 
  exec node  "$basedir/../ejs/bin/cli.js" "$@"
fi
