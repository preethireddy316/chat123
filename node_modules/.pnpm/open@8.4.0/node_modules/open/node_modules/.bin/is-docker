#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/socket-io/client/node_modules/.pnpm/is-docker@2.2.1/node_modules/is-docker/cli.js/node_modules:/home/socket-io/client/node_modules/.pnpm/is-docker@2.2.1/node_modules/is-docker/node_modules:/home/socket-io/client/node_modules/.pnpm/is-docker@2.2.1/node_modules:/home/socket-io/client/node_modules/.pnpm/node_modules:/home/socket-io/client/node_modules:/home/socket-io/node_modules:/home/node_modules:/node_modules:/home/socket-io/client/node_modules/.pnpm/open@8.4.0/node_modules/open/node_modules:/home/socket-io/client/node_modules/.pnpm/open@8.4.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../is-docker@2.2.1/node_modules/is-docker/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../is-docker@2.2.1/node_modules/is-docker/cli.js" "$@"
fi
