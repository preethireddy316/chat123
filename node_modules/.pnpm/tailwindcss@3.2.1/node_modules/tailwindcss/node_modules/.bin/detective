#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/socket-io/client/node_modules/.pnpm/detective@5.2.1/node_modules/detective/bin/detective.js/node_modules:/home/socket-io/client/node_modules/.pnpm/detective@5.2.1/node_modules/detective/bin/node_modules:/home/socket-io/client/node_modules/.pnpm/detective@5.2.1/node_modules/detective/node_modules:/home/socket-io/client/node_modules/.pnpm/detective@5.2.1/node_modules:/home/socket-io/client/node_modules/.pnpm/node_modules:/home/socket-io/client/node_modules:/home/socket-io/node_modules:/home/node_modules:/node_modules:/home/socket-io/client/node_modules/.pnpm/tailwindcss@3.2.1/node_modules/tailwindcss/node_modules:/home/socket-io/client/node_modules/.pnpm/tailwindcss@3.2.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../detective@5.2.1/node_modules/detective/bin/detective.js" "$@"
else 
  exec node  "$basedir/../../../../../detective@5.2.1/node_modules/detective/bin/detective.js" "$@"
fi
